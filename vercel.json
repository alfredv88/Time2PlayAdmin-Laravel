{
  "version": 2,
  "builds": [
    {
      "src": "public/index.php",
      "use": "@vercel/php"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "public/index.php"
    }
  ],
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "APP_NAME": "Time2PlayAdmin",
    "FIREBASE_PROJECT_ID": "time2play-ed370",
    "FIREBASE_CREDENTIALS": "bandmates.json",
    "FIREBASE_DATABASE_URL": "https://time2play-ed370-default-rtdb.firebaseio.com",
    "FIREBASE_STORAGE_DEFAULT_BUCKET": "time2play-ed370.appspot.com"
  },
  "functions": {
    "public/index.php": {
      "runtime": "php8.1"
    }
  }
}
